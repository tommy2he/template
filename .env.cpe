# ========== CPE基本配置 ==========
CPE_ID=cpe-001
CPE_DEVICE_ID=dev-cpe-001
CPE_MANUFACTURER=TP-Link
CPE_MODEL=Archer C7

# ========== ACS服务器配置 ==========
ACS_WS_URL=ws://localhost:7547
ACS_IP=localhost
ACS_UDP_PORT=7548
SERVER_URL=http://localhost:3000

# ========== 心跳配置 ==========
HEARTBEAT_INTERVAL=30

# ========== 设备能力 ==========
CPE_CAPABILITIES=wifi,lan,wan,dhcp

# ========== 模拟配置 ==========
SIMULATE_METRICS=true
METRICS_INTERVAL=60

# ========== WebSocket重连 ==========
WS_RECONNECT_INTERVAL=5000
WS_MAX_RECONNECT_ATTEMPTS=10